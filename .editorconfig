root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.html]
indent_size = 0

[*.ejs]
indent_size = 0

[*.tsx]
indent_size = 4

[*.js]
indent_size = 4

[*.json]
indent_size = 4

[*.md]
indent_size = 4
trim_trailing_whitespace = false
